  <style>
    *,
*:after,
*:before {
  box-sizing: border-box;
}
body {
	display: flex;
	height: 100vh;

	align-items: center;
	justify-content: center;
}

.btn-liquid {
	display: inline-block;
	position: relative;
	width: 100px;
	height: 50px;

	border-radius: 27px;

	color: #fff;
	font: 700 14px/60px "Droid Sans", sans-serif;
	letter-spacing: 0.05em;
	text-align: center;
	text-decoration: none;
	text-transform: uppercase;
}

.btn-liquid .inner {
	position: relative;

	z-index: 2;
}

.btn-liquid canvas {
	position: absolute;
	top: -50px;
	right: -50px;
	bottom: -50px;
	left: -50px;

	z-index: 1;
}
body {
  align-items: center;
  background-color: #C17C74;
  display: flex;
  justify-content: center;
  min-height: 100vh;
}
.duck-loader {
  border: 10px solid #fff;
  border-radius: 100%;
  height: 300px;
  overflow: hidden;
  position: relative;
  transform-origin: preserve-3d;
  width: 300px;
}
.duck-loader__rain {
  position: absolute;
  height: 100%;
  width: 100%;
  top: 0;
  left: 0;
  border-radius: 150px;
  overflow: hidden;
  transform: translate3d(0, 0, 0);
}
.duck-loader__rain .rain__drop {
  height: 30px;
  -webkit-animation-delay: calc(var(--d) * 1s);
          animation-delay: calc(var(--d) * 1s);
  position: absolute;
  left: calc(var(--x) * 1%);
  top: calc((var(--y) + 50) * -1px);
  -webkit-animation-name: drop;
          animation-name: drop;
  -webkit-animation-timing-function: linear;
          animation-timing-function: linear;
  -webkit-animation-iteration-count: infinite;
          animation-iteration-count: infinite;
  -webkit-animation-duration: calc(var(--a) * 1s);
          animation-duration: calc(var(--a) * 1s);
}
.duck-loader__rain .rain__drop path {
  fill: #a1c6cc;
  opacity: var(--o);
  transform: scaleY(calc(var(--s) * 1.5));
}
@-webkit-keyframes drop {
  75% {
    opacity: 1;
  }
  100% {
    transform: translateY(300px);
    opacity: 0;
  }
}
@keyframes drop {
  75% {
    opacity: 1;
  }
  100% {
    transform: translateY(300px);
    opacity: 0;
  }
}
.duck-loader-duck {
  -webkit-clip-path: inset(-100% -100% 25% 0%);
  -webkit-animation: duck 6s infinite;
          animation: duck 6s infinite;
  background: #fff;
  clip-path: inset(-100% -100% 25% 0%);
  height: 15%;
  left: 50%;
  position: absolute;
  top: 50%;
  width: 30%;
}
.duck-loader-duck__wing {
  -webkit-animation: flap calc(var(--wing) / 1000 * 1s) infinite;
          animation: flap calc(var(--wing) / 1000 * 1s) infinite;
  background: #fff;
  height: 50%;
  position: absolute;
  transform-origin: right center;
  width: 50%;
  top: 0;
  left: 0;
}
.duck-loader-duck__head {
  background: #fff;
  bottom: 100%;
  height: 100%;
  position: absolute;
  right: 0;
  width: 50%;
}
.duck-loader-duck__head:before {
  background: #000;
  content: '';
  height: 25%;
  left: 50%;
  position: absolute;
  top: 25%;
  width: 25%;
}
.duck-loader-duck__head:after {
  content: '';
  background: #ffa500;
  height: 35%;
  left: 100%;
  position: absolute;
  top: 40%;
  width: 50%;
}
.duck-loader__pond-water {
  background: #22a7f0;
  border-radius: 100%;
  box-shadow: inset 10px 8px 0 0 #095680;
  height: 85%;
  left: 50%;
  position: absolute;
  transform: translate(-30%, -60%);
  top: 50%;
  width: 110.5%;
}
.duck-loader__pond-water:after {
  background: #095680;
  border-radius: 100%;
  content: '';
  height: 20px;
  left: -9px;
  position: absolute;
  top: 43%;
  width: 20px;
}
.duck-loader__pond {
  background: #2eec71;
  height: 100%;
  width: 100%;
}
.duck-loader__pond:before,
.duck-loader__pond:after {
  content: '';
  background-color: #22a7f0;
  position: absolute;
  left: 50%;
  top: 50%;
}
.duck-loader__pond:after {
  -webkit-clip-path: inset(0 50% 0 0);
  background: #22a7f0;
  border-radius: 100%;
  clip-path: inset(0 50% 0 0);
  height: 65%;
  position: absolute;
  transform: translate(-65%, -15%) rotate(15deg);
  width: 97.5%;
}
.duck-loader__pond:before {
  border-radius: 100%;
  box-shadow: inset 10px 10px 0 0 #095680;
  height: 75%;
  transform: translate(-70%, -20%);
  width: 112.5%;
}
.mute {
  background: transparent;
  border: 0;
  bottom: 10px;
  cursor: pointer;
  position: fixed;
  right: 10px;
}
.mute--active:after {
  background: #C17C74;
  content: '';
  height: 100%;
  left: 50%;
  position: absolute;
  top: 0%;
  width: 50%;
}
.mute__icon {
  height: 44px;
  width: 44px;
}
@-webkit-keyframes duck {
  0%, 85%, 95%, 100% {
    transform: translate(-50%, 0) rotateY(0);
  }
  15% {
    transform: translate(25%, 0) rotateY(0);
  }
  20%, 25% {
    transform: translate(25%, 0) rotateY(180deg);
  }
  40%, 45% {
    transform: translate(-50%, 0) rotateY(180deg);
  }
  60% {
    transform: translate(-125%, 0) rotateY(180deg);
  }
  65%, 70% {
    transform: translate(-125%, 0) rotateY(0deg);
  }
}
@keyframes duck {
  0%, 85%, 95%, 100% {
    transform: translate(-50%, 0) rotateY(0);
  }
  15% {
    transform: translate(25%, 0) rotateY(0);
  }
  20%, 25% {
    transform: translate(25%, 0) rotateY(180deg);
  }
  40%, 45% {
    transform: translate(-50%, 0) rotateY(180deg);
  }
  60% {
    transform: translate(-125%, 0) rotateY(180deg);
  }
  65%, 70% {
    transform: translate(-125%, 0) rotateY(0deg);
  }
}
@-webkit-keyframes flap {
  0%, 92%, 100% {
    transform: rotate(0deg);
  }
  96% {
    transform: rotate(60deg);
  }
}
@keyframes flap {
  0%, 92%, 100% {
    transform: rotate(0deg);
  }
  96% {
    transform: rotate(60deg);
  }
}
